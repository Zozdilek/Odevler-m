aylikgelir=float(input("aylikmgelir"))
mutfak=float(input("mutfak"))
egitim=float(input("egitim"))
ulasim=float(input("ulasim"))
giyim=float(input("giyim"))
masraf=mutfak+egitim+ulasim+giyim
print("sonuç",masraf,aylikgelir/masraf)
f=open("aylıkmasraf.txt","w")
f.write(str(masraf))
f.write (str(mutfak))
f.write (str(egitim))
f.write(str(ulasim))
f.write(str(giyim))
f.close()
